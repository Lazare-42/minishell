                <script type="text/javascript">
    <!--
    function getCookie(e){
      if(document.cookie.length == 0){
        return 0
      }
      var t=new RegExp('(; )','g');
      var n=document.cookie.split(t);

      for(var r=0;r<n.length;r++){
        var i=new RegExp('=','g');
        var s=n[r].split(i);
        if(s[0]==e){
          return unescape(s[1])
        }
      }
      return ''
    };
    var initialXtparam = window.xtparam || '';

    function updateXtparam() {
      window.xt_ac = getCookie('userstatus');
      if (window.xt_ac == 0) window.xt_ac = '';
