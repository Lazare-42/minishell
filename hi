      window.wallpaperAdCallback = function(height) {
        var flashes = document.querySelectorAll('.gutter .strip');
        var offset = 0;
        var gutter;
        Array.prototype.forEach.call(flashes, function(flash){
          offset += flash.offsetHeight;
        });
        if(flashes.length > 0)
          gutter = document.querySelector('.gutter + .gutter')
        else
          gutter = document.querySelector('.gutter')

        gutter.style.marginTop = height + 'px';

        document.body.style.backgroundPosition = 'center ' + offset + 'px';
        document.body.style.cursor = 'normal';

        Array.prototype.forEach.call(document.querySelectorAll('[id^="takeover_"]'), function(ad){
          ad.style.top = offset + 'px';
        })
      };
